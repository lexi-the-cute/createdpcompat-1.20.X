{
  "schemaVersion": 1,
  "id": "createdpcompat",
  "version": "${version}",
  "name": "Create compat for datapack portals",
  "description": "This mod ensures that create trains can pass through portals from the datapack portals mod",
  "authors": [
    "Misigno",
    "lexi-the-cute"
  ],
  "license": "MIT",
  "icon": "assets/createdpcompat/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.misigno.createdpcompat.CreateDPCompat"
    ],
    "client": [
      "net.misigno.createdpcompat.CreateDPCompatClient"
    ]
  },
  "mixins": [
    "createdpcompat.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "*",
    "minecraft": "${minecraft_version}",
    "datapackportals": ">=${datapackportals_version}"
  }
}
